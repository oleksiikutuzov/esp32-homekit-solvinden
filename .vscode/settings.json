{
	"C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
	"cSpell.words": [
		"Berman",
		"CHPD",
		"Colorwheel",
		"Erriez",
		"Espressif",
		"homekit",
		"LEDS",
		"manu",
		"MAXHUE",
		"millis",
		"Mosfet",
		"NEOPIXEL",
		"RGBW",
		"SOLVINDEN",
		"togther",
		"unsoliciated",
		"VINDRIKTNING",
		"Wemos",
		"WROOM"
	],
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"editor.matchBrackets": "near",
	"C_Cpp.enhancedColorization": "Enabled",
	"arduino.additionalUrls": [
		"https://raw.githubusercontent.com/espressif/arduino-esp32/gh-pages/package_esp32_index.json"
	],
	"editor.fontFamily": "Fira Code",
	"editor.fontLigatures": true,
	"files.associations": {
		"array": "cpp",
		"associative_base": "cpp",
		"deque": "cpp",
		"initializer_list": "cpp",
		"iterator": "cpp",
		"list": "cpp",
		"serstream": "cpp",
		"vector": "cpp",
		"*.ipp": "cpp",
		"string": "cpp",
		"unordered_map": "cpp",
		"unordered_set": "cpp",
		"string_view": "cpp",
		"regex": "cpp",
		"valarray": "cpp",
		"cstddef": "cpp",
		"random": "cpp",
		"ios": "cpp",
		"*.tcc": "cpp",
		"bitset": "cpp",
		"set": "cpp"
	},
	"VsCodeTaskButtons.tasks": [
		{
			"label": "Erase Flash",
			"task": "Erase Flash"
		}
	],
	"cSpell.enableFiletypes": ["!yaml"],
	"yaml.schemas": {
		"https://json.schemastore.org/github-workflow.json": "file:///Users/user/git/esp32-homekit-led-strip/.github/workflows/build.yml"
	},
	"arduino.path": "/Applications", // /usr/local/bin/
	"arduino.commandPath": "Contents/MacOS/Arduino", // arduino-cli
	"arduino.useArduinoCli": false,
	"arduino.defaultTimestampFormat": "[%H:%M:%S] "
}
